# https://kubernetes.io/docs/concepts/configuration/secret/

kubectl -n default create secret generic mysql-pass --from-literal=username=root --from-literal=password='S!B\*d$zDsb='

kubectl -n default create secret generic mysql-pass --from-literal=password='123456789'